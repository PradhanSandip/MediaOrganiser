<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox maxHeight="1.7976931348623157E308" maxWidth="220.0" minHeight="-Infinity" minWidth="-Infinity" prefWidth="220.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.java.project.MediaOrganiserProgram.PlaylistOptions">
   <children>
      <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" spacing="10.0" VBox.vgrow="ALWAYS">
         <children>
            <ImageView fx:id="folder_logo" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../../../../../../../Downloads/red_folder.png" />
               </image>
            </ImageView>
            <Label fx:id="playlist_name" text="Label">
               <font>
                  <Font size="26.0" />
               </font>
            </Label>
            <Label fx:id="file_count" text="Label">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
            <Button fx:id="import_file_button" mnemonicParsing="false" prefWidth="200.0" text="Import files">
               <font>
                  <Font size="19.0" />
               </font>
            </Button>
            <Button fx:id="export_button" mnemonicParsing="false" prefWidth="200.0" text="Export playlist">
               <font>
                  <Font size="19.0" />
               </font>
            </Button>
            <Button fx:id="delete_button" layoutX="165.0" layoutY="421.0" mnemonicParsing="false" prefWidth="200.0" text="Delete">
               <font>
                  <Font size="19.0" />
               </font>
            </Button>
         </children>
      </VBox>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="20.0" VBox.vgrow="SOMETIMES">
         <children>
            <Button fx:id="import_button" mnemonicParsing="false" prefWidth="200.0" text="Import playlist">
               <font>
                  <Font size="19.0" />
               </font>
            </Button>
            <Button fx:id="create_button" mnemonicParsing="false" prefWidth="200.0" text="Create new playlist">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
         </children>
      </VBox>
   </children>
</VBox>
